{
    "manifest_version": 3,
    "name": "Tab Search",
    "version": "1.0",
    "description": "Search for tabs.",
    "permissions": [
      "tabs",
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",

      "default_icon": "tabfinder.png"

    }
  }
  